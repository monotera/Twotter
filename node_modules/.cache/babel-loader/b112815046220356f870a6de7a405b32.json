AAAA,oBAEF,MAAA,CAAA,sBAAA,GAAsB;AAFpB,KAArB,CAuBA;AAtBJ,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,MAAA,CAAA,cAAA,IAAA,MAAA,CAAA,cAAA,OAAA,MAAA,YAAV;AAAA,KAAA,EAAwB,CAAA,SAAA,CAAxB,CAAA;AAsBF,GAxBP,E,CAKE,YAAA,CAEC,OAFD,EAAA,UAAA,EAEC,CADE,UACF,E,iBAD4B,OAAE,gBAAA,CAAG,MAAA,CAAA,sBAAH,CAAF,GAA8B,O,EAAK,C,CAC/D,CAFD,C,kBAGA,YAAA,CAAmE,UAAnE,EAAmE;AAAzD,IAAA,EAAE,EAAC,UAAsD;AAA3C,IAAA,IAAI,EAAC,GAAsC;;aAAzB,MAAA,CAAA,KAAA,CAAM,eAAN,GAAqB,M;;AAAI,GAAnE,E,IAAA,E,GAAA,C,iBAA0C,MAAA,CAAA,KAAA,CAAM,e,KAChD,YAAA,CAcM,KAdN,EAAA,UAAA,EAcM,CAbJ,YAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,UAUI,E,gBATJ,YAAA,CAQS,QART,EAQS;AARD,IAAA,EAAE,EAAC,cAQF;;aAR0B,MAAA,CAAA,KAAA,CAAM,iBAAN,GAAuB,M;;AAQjD,GART,E,oBACE,YAAA,CAMS,SANT,EAMS,IANT,EAMS,WAAA,CAJmB,MAAA,CAAA,KAAA,CAAM,SAIzB,EAJkC,UAAjC,MAAiC,EAAzB,KAAyB,EAApB;yBAFvB,YAAA,CAMS,QANT,EAMS;AALN,MAAA,KAAK,EAAE,MAAM,CAAC,KAKR;AAHN,MAAA,GAAG,EAAE;AAGC,KANT,E,iBAKK,MAAM,CAAC,I,CALZ,EAKgB,CALhB,EAKgB,CAAA,OAAA,CALhB,C;GAMS,CANT,E,GAAA,C,EADF,E,GAAA,C,mBAAmC,MAAA,CAAA,KAAA,CAAM,iB,GASrC,CAXN,CAaI,EADJ,UACI,CAdN,C,CATF,E,EAAA,C","sourcesContent":["<template>\n  <form\n    class=\"create-twoot-panel\"\n    @submit.prevent=\"createNewTwoot\"\n    :class=\"{ '--exceeded': newTwootCharacterCount > 180 }\"\n  >\n    <label for=\"newTwoot\"\n      ><strong>New Twoot</strong> ({{ newTwootCharacterCount }}/180)</label\n    >\n    <textarea id=\"newTwoot\" rows=\"4\" v-model=\"state.newTwootContent\" />\n    <div class=\"create-twoot-panel__submit\">\n      <div class=\"create-twoot-type\">\n        <label for=\"newTwootType\"><strong>Type: </strong></label>\n        <select id=\"newTwootType\" v-model=\"state.selectedTwootType\">\n          <option\n            :value=\"option.value\"\n            v-for=\"(option, index) in state.twootType\"\n            :key=\"index\"\n          >\n            {{ option.name }}\n          </option>\n        </select>\n      </div>\n      <button>Twoot It!!</button>\n    </div>\n  </form>\n</template>\n\n<script>\nimport { reactive, computed } from \"vue\";\nexport default {\n  name: \"CreateTwootPanel\",\n  setup(props, ctx) {\n    const state = reactive({\n      newTwootContent: \"\",\n      selectedTwootType: \"instant\",\n      twootType: [\n        { value: \"draft\", name: \"Draft\" },\n        { value: \"instant\", name: \"Instant Twoot\" },\n      ],\n    });\n\n    const newTwootCharacterCount = computed(() => state.newTwootContent.length);\n\n    function createNewTwoot() {\n      if (state.newTwootContent && state.selectedTwootType !== \"draft\") {\n        ctx.emit(\"add-twoot\", state.newTwootContent);\n        state.newTwootContent = \"\";\n      }\n    }\n    return {\n      state,\n      newTwootCharacterCount,\n      createNewTwoot,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.create-twoot-panel {\n  padding: 1.5rem 0;\n  display: flex;\n  flex-direction: column;\n  textarea {\n    border: 1px solid #dfe3e8;\n    border-radius: 5px;\n  }\n  .create-twoot-panel__submit {\n    display: flex;\n    flex-direction: column;\n    .create-twoot-type {\n      padding: 1rem 0;\n    }\n    button {\n      padding: 5px 20px;\n      margin: auto 0;\n      border-radius: 5px;\n      border: none;\n      background-color: lightseagreen;\n      color: white;\n      font-weight: bold;\n    }\n  }\n  select {\n    border-radius: 5px;\n  }\n  &.--exceeded {\n    color: red;\n    border-color: red;\n    .create-twoot-panel__submit {\n      button {\n        background-color: red;\n        color: white;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}